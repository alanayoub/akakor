{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./src/main-electron.js",
  "scripts": {
    "start": "electron .",
    "dev": "npm run watch-js & npm run watch-css",
    "watch-js": "watchify --insert-globals true ./src/main-web.js --debug -o ./src/main-web-build.js",
    "watch-css": "sass --watch ./src/styles"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Alan Ayoub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babelify": "^7.3.0",
    "electron-prebuilt-compile": "^1.4.15",
    "firebase": "^3.7.3",
    "firebaseui": "^1.0.0",
    "golden-layout": "^1.5.7",
    "jquery": "^3.2.1",
    "vue": "^2.2.4",
    "vue-form-generator": "^2.0.0-beta5",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
